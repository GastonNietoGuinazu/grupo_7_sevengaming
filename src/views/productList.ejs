<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css"
    />
    <script src="https://kit.fontawesome.com/6d7ddf6bb9.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/css/producto.css" />
    <link rel="stylesheet" href="/css/home.css" />
    <link rel="stylesheet" href="/css/cuenta.css" />
    <link rel="stylesheet" href="/css/productList.css" />
    <title>Product List</title>
  </head>
  <body>
    <!-- Header -->
    <%- include('partials/header') %>

    <!---PRIMERA SECCIÓN-->

    <article class="juegos">
      <div class="titleContainer"><span class="tituloJuegos">Juegos</span></div>
      <section class="gamesContainer">
        <div class="juego1">
          <img class="img-game" src="img/fortnite-1.webp" alt="juego" />
        <h3 class="title1">FORTNITE</h3>
        </div>
        <div class="juego1">
          <img class="img-game" src="img/hogwarts.jpg" alt="juego" />
          <h3 class="title1">HOGWARTS</h3>
        </div>
        <div class="juego1">
          <img class="img-game" src="img/world-war.webp" alt="juego" />
          <h3 class="title1">HEROES</h3>
        </div>
        <div class="juego1">
          <img class="img-game" src="img/horizon.jpg" alt="juego" />
          <h3 class="title1">HORIZON</h3>
        </div>
      </section>
    </article>

    <!--SEGUNDA SECCIÓN-->
    <article class="libros">
        <h3 class="tituloLibros">Libros y Arte</h3>
        <section class="gamesContainer">
          <div class="libro1">
            <img class="img-game" src="img/halo.jpg" alt="libro" />
          <h3 class="title2">HALO</h3>
          </div>
          <div class="libro1">
            <img class="img-game" src="img/harry-potter.jpg" alt="libro" />
            <h3 class="title2">HARRY POTTER</h3>
          </div>
          <div class="libro1">
            <img class="img-game" src="img/player-one.png" alt="libro" />
            <h3 class="title2">READY PLAYER ONE</h3>
          </div>
          <div class="libro1">
            <img class="img-game" src="img/star-wars.webp" alt="libro" />
            <h3 class="title2">STAR WARS</h3>
          </div>
        </section>
      </article>
    
      <!--TERCERA SECCIÓN-->
      <article class="pins">
        <h3 class="tituloPins">Pins</h3>
        <section class="gamesContainer">
          <div class="pin1">
            <img class="img-game" src="img/pin-destiny.jpg" alt="pin" />
          <h3 class="title3">DESTINY</h3>
          </div>
          <div class="pin1">
            <img class="img-game" src="img/pin-halo.jpg" alt="pin" />
            <h3 class="title3">HALO</h3>
          </div>
          <div class="pin1">
            <img class="img-game" src="img/pin-harry.jpg" alt="pin" />
            <h3 class="title3">HARRY POTTER</h3>
          </div>
          <div class="pin1">
            <img class="img-game" src="img/pin-star-wars.jpg" alt="pin" />
            <h3 class="title3">STAR WARS</h3>
          </div>
        </section>
      </article>
    
    <!--CUARTA SECCIÓN-->
    <article class="accesorios">
        <h3 class="tituloAccesorios">Accesorios</h3>
        <section class="gamesContainer">
          <div class="accesorios1">
            <img class="img-game" src="img/auriculares.jpg" alt="accesorio" />
          <h3 class="title4">AURICULARES</h3>
          </div>
          <div class="accesorios1">
            <img class="img-game" src="img/gorra-fortnite.jpg" alt="accesorio" />
            <h3 class="title4">GORRA FORTNITE</h3>
          </div>
          <div class="accesorios1">
            <img class="img-game" src="img/joystick.jpg" alt="accesorio" />
            <h3 class="title4">JOYSTICK</h3>
          </div>
          <div class="accesorios1">
            <img class="img-game" src="img/mochila-gaming.webp" alt="accesorio" />
            <h3 class="title4">MOCHILA GAMING</h3>
          </div>
        </section>
      </article>  
    
    <!-- Footer -->
    <%- include('partials/footer') %>
  </body>
</html>
